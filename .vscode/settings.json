{
    "cSpell.words": [
      "headshot",
      "tailwindcss"
    ],
    "files.autoSave": "onFocusChange",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false,
    "javascript.format.enable": false,
    "eslint.options": {
      "extensions": [ ".html", ".js", ".vue", ".jsx", ".ts" ]
    },
    "eslint.validate": [
      "html",
      "javascript",
      "Angular"
    ],
}